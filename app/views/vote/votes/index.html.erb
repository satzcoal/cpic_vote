<h1>Listing vote_votes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start time</th>
      <th>Stop time</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_votes.each do |vote_vote| %>
      <tr>
        <td><%= vote_vote.name %></td>
        <td><%= vote_vote.start_time %></td>
        <td><%= vote_vote.stop_time %></td>
        <td><%= vote_vote.url %></td>
        <td><%= link_to 'Show', vote_vote %></td>
        <td><%= link_to 'Edit', edit_vote_vote_path(vote_vote) %></td>
        <td><%= link_to 'Destroy', vote_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_vote_path %>
